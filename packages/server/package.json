{
  "name": "sliver-builder-server",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "dev": "nodemon --inspect ./src/server.ts",
    "start": "npm run serve",
    "build": "npm run build-sass && npm run build-ts && npm run lint && npm run copy-static-assets",
    "serve": "node dist/server.js",
    "watch-node": "nodemon dist/server.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"Sass,TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-sass\" \"npm run watch-ts\" \"npm run watch-node\"",
    "test": "jest --forceExit --coverage --verbose",
    "watch-test": "npm run test -- --watchAll",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "build-sass": "node-sass src/public/css/main.scss dist/public/css/main.css",
    "watch-sass": "node-sass -w src/public/css/main.scss dist/public/css/main.css",
    "lint": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix",
    "copy-static-assets": "ts-node copyStaticAssets.ts",
    "debug": "npm run build && npm run watch-debug",
    "serve-debug": "nodemon --inspect dist/server.js",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"Sass,TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-sass\" \"npm run watch-ts\" \"npm run serve-debug\""
  },
  "author": "Joel TROUSSET",
  "license": "ISC",
  "dependencies": {
    "@types/cors": "^2.8.6",
    "@types/express-http-proxy": "^1.5.12",
    "bluebird": "^3.7.1",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "errorhandler": "^1.5.1",
    "express": "^4.17.1",
    "express-http-proxy": "^1.6.0",
    "mongoose": "^5.7.9",
    "node-cache": "^5.0.1",
    "scryfall-sdk-jtro": "^1.5.1",
    "sliver-builder-common": "1.0.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/async": "^3.0.2",
    "@types/bluebird": "^3.5.27",
    "@types/dotenv": "^8.2.0",
    "@types/errorhandler": "^0.0.32",
    "@types/eslint": "^6.1.1",
    "@types/express": "^4.17.1",
    "@types/lodash": "^4.14.141",
    "@types/mongoose": "^5.5.18",
    "@types/winston": "^2.4.4",
    "concurrently": "^4.1.2",
    "eslint": "^6.4.0",
    "nodemon": "^1.19.4",
    "ts-node": "^8.4.1",
    "ts-node-dev": "^1.0.0-pre.44",
    "typescript": "^3.7.2"
  }
}