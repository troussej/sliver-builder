{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-config-airbnb"
  ],
  "jsRules": {
    "no-unused-expression": true
  },
  "rules": {
    "max-line-length": [
      true,
      130
    ],
    "interface-name": [
      true,
      "never-prefix"
    ],
    "import-name": false,
    "no-console": false,
    "object-literal-sort-keys": false,
    "no-any": true,
    "no-unused-expression": true,
    "typedef": true,
    "space-before-function-paren": [
      true,
      {
        "constructor": "never"
      }
    ]
  },
  "rulesDirectory": []
}